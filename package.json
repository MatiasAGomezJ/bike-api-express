    },
    "dependencies": {
        "cookie-parser": "~1.4.4",
        "debug": "~2.6.9",
        "express": "~4.16.1",
        "http-errors": "~1.6.3",
        "jade": "~1.11.0",
        "mongoose": "^6.8.3",
        "morgan": "~1.9.1"
    },
    "devDependencies": {
        "jest": "^29.3.1",
        "nodemon": "^2.0.20",
        "supertest": "^6.3.3"
    }
}
